    <div class="line5"></div>

      <div class="fbox1">

        <canvas id="myChart2" ></canvas>
      </div>
      <div class="line5"></div>

    <script src="chart.js"></script>
    <script>
    var barColors = ["red", "green", "blue", "orange", "brown"];


    var xValues2 = ["CodeSniffer", "Axe Core", "Alfa"];
    var yValues2 = [<%= html_code_sniffer %>, 20, 24, 0];


    var xValues4 = ["Included in code", "Not included in code"];
    var yValues4 = [<%= html_code_sniffer_per %>  , 100 -<%= html_code_sniffer_per %>];

    var xValues5 = ["Included in code", "Not included in code"];
    var yValues5 = [32.4, 100-32.4];

    var xValues6 = ["Included in code", "Not included in code"];
    var yValues6 = [29.8, 100-29.8];


     
    new Chart("myChart2", {
      type: "bar",
      data: {
        labels: xValues2,
        datasets: [{
          backgroundColor: barColors,
          data: yValues2
        }]
      },
      options: {
        legend: { display: false },
        title: {
          display: true,
          text: "Score Comparsion"
        }
      }
    })

  


    new Chart("myChart4", {
      type: "pie",
      data: {
        labels: xValues4,
        datasets: [{
          backgroundColor: barColors,
          data: yValues4
        }]
      },
      options: {
        legend: { display: false },
        title: {
          display: true,
          text: "CodeSniffer score percentage"
        }
      }
    })

    new Chart("myChart5", {
      type: "pie",
      data: {
        labels: xValues5,
        datasets: [{
          backgroundColor: barColors,
          data: yValues5
        }]
      },
      options: {
        legend: { display: false },
        title: {
          display: true,
          text: "Axe Core score percentage"
        }
      }
    })

    new Chart("myChart6", {
      type: "pie",
      data: {
        labels: xValues6,
        datasets: [{
          backgroundColor: barColors,
          data: yValues6
        }]
      },
      options: {
        legend: { display: false },
        title: {
          display: true,
          text: "Alfa score percentage"
        }
      }
    })

    </script>











